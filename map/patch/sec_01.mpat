#new:Script $Script_NodeModifiers
{
    If *MF_AnomaliesPlus == .True
        Add *Var0 10`
        If *Var0 > 100`
            Set *Var0 100`
        EndIf
    EndIf
    If *MF_AnomaliesMinus == .True
        Sub *Var0 10`
        If *Var0 < 0`
            Set *Var0 0`
        EndIf
    EndIf
    If *MF_OopsAllAnomalies == .True
        Set *Var0 100`
    EndIf
    Return
    End
}

