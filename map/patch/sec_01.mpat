    Switch *Var0
    Case 0` to 100`
        Call  ShowMessageAtScreenPos    ( 002F001B  160`  40` ) % No Encounter!
    EndSwitch
    Exec $Script_Main_Callback_AfterEnterMap
    Return
    End
}

#new:Script $Script_NextSector
{
    Add *MB_SectorsCompleted 1`
    Call  PlayerMoveTo ( 800` 0` 40` )
    Wait  20`
    If *MB_SectorsCompleted == 4`
        Set *VarF 002F0024
        Goto 2
    EndIf
    Label 0
    Wait 1`
    Call  RandInt ( 4` *Var0 )
    Add *Var0 1`
    Wait 1`
    Switch *Var0
    Case == 1`
        If *MF_SectorSeenKoopa == .False
            Set *VarF 002F001F
            Set *MF_SectorSeenKoopa .True
        Else
            Goto 0
        EndIf
    Case == 2`
        If *MF_SectorSeenGearmos == .False
            Set *VarF 002F0020
            Set *MF_SectorSeenGearmos .True
        Else
            Goto 0
        EndIf
    Case == 3`
        If *MF_SectorSeenVoid == .False
            Set *VarF 002F0021
            Set *MF_SectorSeenVoid .True
        Else
            Goto 0
        EndIf
    Case == 4`
        If *MF_SectorSeenBowser == .False
            Set *VarF 002F0022
            Set *MF_SectorSeenBowser .True
        Else
            Goto 0
        EndIf
    Case == 5`
        If *MF_SectorSeenPirates == .False
            Set *VarF 002F0023
            Set *MF_SectorSeenPirates .True
        Else
            Goto 0
        EndIf
    EndSwitch
    Call  SetMessageString ( *VarF 00000000 )
    Label 1
    Wait 1`
    Call  RandInt ( 4` *Var0 )
    Add *Var0 1`
    Wait 1`
    Switch *Var0
    Case == 1`
        If *MF_SectorSeenKoopa == .False
            Set *VarE 002F001F
            Set *MF_SectorSeenKoopa .True
        Else
            Goto 1
        EndIf
    Case == 2`
        If *MF_SectorSeenGearmos == .False
            Set *VarE 002F0020
            Set *MF_SectorSeenGearmos .True
        Else
            Goto 1
        EndIf
    Case == 3`
        If *MF_SectorSeenVoid == .False
            Set *VarE 002F0021
            Set *MF_SectorSeenVoid .True
        Else
            Goto 1
        EndIf
    Case == 4`
        If *MF_SectorSeenBowser == .False
            Set *VarE 002F0022
            Set *MF_SectorSeenBowser .True
        Else
            Goto 1
        EndIf
    Case == 5`
        If *MF_SectorSeenPirates == .False
            Set *VarE 002F0023
            Set *MF_SectorSeenPirates .True
        Else
            Goto 1
        EndIf
    EndSwitch
    Call  SetMessageString ( *VarE 00000001 )
    Call  ShowMessageAtScreenPos    ( 002F001C  160`  40` ) % Next Sector!
    Call  ShowChoice ( 002F001D )
    If *Var0 == 00000000
        Call CloseMessage ()
        Switch *VarF
        Case == 002F001F
            Call GotoMapSpecial ( "sec_02" ~Entry:sec_02:SectorStart 0000000D )    % Koopa Dominion!
        Case == 002F0020
            Call GotoMapSpecial ( "sec_03" ~Entry:sec_03:SectorStart 0000000D )    % Gearmos Homeworlds!
        Case == 002F0021
            Call GotoMapSpecial ( "sec_04" ~Entry:sec_04:SectorStart 0000000D )    % Void of Space!
        Case == 002F0022
            Call GotoMapSpecial ( "sec_05" ~Entry:sec_05:SectorStart 0000000D )    % Bowser's Space Armada!
        Case == 002F0023
            Call GotoMapSpecial ( "sec_06" ~Entry:sec_06:SectorStart 0000000D )    % Space Pirate Sector!
        EndSwitch
    Else
        Call CloseMessage ()
        Switch *VarE
        Case == 002F001F
            Call GotoMapSpecial ( "sec_02" ~Entry:sec_02:SectorStart 0000000D )    % Koopa Dominion!
        Case == 002F0020
            Call GotoMapSpecial ( "sec_03" ~Entry:sec_03:SectorStart 0000000D )    % Gearmos Homeworlds!
        Case == 002F0021
            Call GotoMapSpecial ( "sec_04" ~Entry:sec_04:SectorStart 0000000D )    % Void of Space!
        Case == 002F0022
            Call GotoMapSpecial ( "sec_05" ~Entry:sec_05:SectorStart 0000000D )    % Bowser's Space Armada!
        Case == 002F0023
            Call GotoMapSpecial ( "sec_06" ~Entry:sec_06:SectorStart 0000000D )    % Space Pirate Sector!
        EndSwitch
    EndIf
    Return
    Label 2
    Call  SetMessageString ( *VarF 00000000 )
    Call  ShowMessageAtScreenPos    ( 002F001C  160`  40` ) % Next Sector!
    Call  ShowChoice ( 002F005A )
    Call CloseMessage ()
    Call GotoMapSpecial ( "sec_07" ~Entry:sec_07:SectorStart 0000000D )    % Tatanga's Nebula!
    Return
    End
}